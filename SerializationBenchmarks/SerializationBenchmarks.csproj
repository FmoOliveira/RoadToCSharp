<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>
  <ItemGroup>
    <Protobuf Include="Protos\user.proto" GrpcServices="None" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Amazon.IonDotnet" Version="1.3.1" />
    <PackageReference Include="Apache.Avro" Version="1.12.0" />
    <PackageReference Include="BenchmarkDotNet" Version="0.15.0" />
    <PackageReference Include="MessagePack" Version="3.1.3" />
    <PackageReference Include="Google.Protobuf" Version="3.25.1" />
    <PackageReference Include="Grpc.Tools" Version="2.61.0" PrivateAssets="All" />
    <PackageReference Include="MongoDB.Bson" Version="3.4.0" />
    <PackageReference Include="System.Formats.Cbor" Version="9.0.5" />
  </ItemGroup>

</Project>
